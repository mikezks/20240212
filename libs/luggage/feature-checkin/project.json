{
  "name": "luggage-feature-checkin",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/luggage/feature-checkin/src",
  "prefix": "luggage",
  "tags": ["domain:luggage", "type:feature"],
  "projectType": "library",
  "targets": {
    "test": {
